{
	"id": "uql.terminus",
	"name": "Local Terminus",
	"version": "0.1.22",
	"authors": "upquilled",
	"description": "Venture beyond the Chasing Wind Superstructure",
	"requirements": [ "myr.chasing_wind", "uql.extra" ],
	"requirements_names": [ "Chasing Wind", "Extra Parameters" ],
	"tags": [ "Regions" ],
	"checksum_override_version": true,
	"uql.extra": {
		"exclude_steam": {
			"globs": [
				"fairypresets/*",
				"source/**",
				"lightrooms/*",
				".git/**",
				".gitattributes",
				".gitignore",
				"**/.DS_Store",
				"README.md",
				"**/Thumbs.db",
				"**.tmp",
				"bwu.json",
				"plugins/**.pdb",
				"world/jx/map_jx_backup.png",
				"music/**",
				"!music/songs/jx-capricorn.ogg",
				"!music/songs/jx-suntimer.ogg",
				"!music/songs/jx-halcyon*.ogg",
				"!music/songs/*.txt"
			],
			"delete_temp": true
		}
	}
}